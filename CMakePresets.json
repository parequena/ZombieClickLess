{
   "version": 5,
   "cmakeMinimumRequired": {
      "major": 3,
      "minor": 25,
      "patch": 0
   },
   "include": [
      "cmake/DebugPresets.json",
      "cmake/ReleasePresets.json",
      "cmake/ClangPresets.json",
      "cmake/WindowsPresets.json"
   ],
   "configurePresets": [
      {
         "name": "clang-debug",
         "displayName": "Clang Debug",
         "description": "",
         "inherits": [
            "debug-cfg",
            "clang-cfg"
         ]
      },
      {
         "name": "clang-release",
         "displayName": "Clang Release",
         "description": "",
         "inherits": [
            "release-cfg",
            "clang-cfg"
         ]
      },
      {
         "name": "windows-debug",
         "displayName": "Windows Debug",
         "description": "",
         "inherits": [
            "debug-cfg",
            "windows-cfg"
         ]
      },
      {
         "name": "windows-release",
         "displayName": "Windows Release",
         "description": "",
         "inherits": [
            "release-cfg",
            "windows-cfg"
         ]
      }
   ]
}
